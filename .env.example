# 数据库配置
POSTGRES_DB=hackathon
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password

# 数据库连接URL
DATABASE_URL=postgresql://postgres:your_secure_password@postgres:5432/hackathon

# API URLs (生产环境 - 直接使用服务端口)
NEXT_PUBLIC_API_URL=http://your-server-ip:3001
NEXT_PUBLIC_HONO_API_URL=http://your-server-ip:3002
VITE_API_URL=http://your-server-ip:3001
VITE_HONO_API_URL=http://your-server-ip:3002

# 环境
NODE_ENV=production